	year = {2024},
	journal = {2024 IEEE International Conference on Metrology for eXtended Reality, Artificial Intelligence and Neural Engineering, MetroXRAINE 2024 - Proceedings},
	pages = {142 – 147},
	doi = {10.1109/MetroXRAINE62247.2024.10797001},
