@ARTICLE{Riedmann-Streitz2024,
	author = {Riedmann-Streitz, Christine and Streitz, Norbert and Antona, Margherita and Marcus, Aaron and Margetis, George and Ntoa, Stavroula and Rau, Pei-Luen Patrick and Rosenzweig, Elizabeth},
