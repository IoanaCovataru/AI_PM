@ARTICLE{Peng2021e250,
	author = {Peng, Sui and Liu, Yihao and Lv, Weiming and Liu, Longzhong and Zhou, Qian and Yang, Hong and Ren, Jie and Liu, Guangjian and Wang, Xiaodong and Zhang, Xuehua and Du, Qiang and Nie, Fangxing and Huang, Gao and Guo, Yuchen and Li, Jie and Liang, Jinyu and Hu, Hangtong and Xiao, Han and Liu, Zelong and Lai, Fenghua and Zheng, Qiuyi and Wang, Haibo and Li, Yanbing and Alexander, Erik K and Wang, Wei and Xiao, Haipeng},
	title = {Deep learning-based artificial intelligence model to assist thyroid nodule diagnosis and management: a multicentre diagnostic study},
